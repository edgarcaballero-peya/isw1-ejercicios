	|summary|
	
	summary _ OrderedCollection new.
	accounts do: [:anAccount |
		(anAccount isKindOf: ReceptiveAccount) ifTrue: [
			(anAccount isKindOf: Deposit) ifTrue: [summary add: 'Depósito por ', anAccount value asString, '.'].
			(anAccount isKindOf: Withdraw ) ifTrue: [summary add: 'Extracción por ', anAccount value asString, '.'].
			(anAccount isKindOf: WithdrawTransfer ) ifTrue: [summary add: 'Salida por transferencia de ', anAccount transfer value asString, '.'].
			(anAccount isKindOf: DepositTransfer ) ifTrue: [summary add: 'Entrada por transferencia de ', anAccount transfer value asString, '.'].	
			]
	].
	summary add: 'Balance = ', self balance asString.
	
	^summary .! !
